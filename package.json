{
  "scripts": {
    "build": "zile",
    "clean": "tsc -b --clean && rm -rf dist",
    "changeset:publish": "zile publish:prepare && changeset publish && zile publish:post",
    "changeset:version": "changeset version",
    "check": "biome check --fix --unsafe",
    "check:types": "tsc -b",
    "dev": "zile dev",
    "dev:playground": "pnpm --filter playground dev",
    "test": "vitest ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@changesets/cli": "catalog:",
    "@types/estree": "^1.0.8",
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/mdx": "^2.0.13",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "astring": "^1.9.0",
    "react": "catalog:",
    "simple-git-hooks": "catalog:",
    "tsx": "^4.21.0",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:",
    "waku": "catalog:",
    "zile": "catalog:"
  },
  "packageManager": "pnpm@10.25.0",
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": "24.12.0",
      "onFail": "download"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm check"
  },
  "[!start-pkg]": "",
  "name": "vocs",
  "type": "module",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@base-ui/react": "^1.0.0",
    "@codesandbox/sandpack-react": "^2.20.0",
    "@iconify/utils": "^3.1.0",
    "@mdx-js/mdx": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@mdx-js/rollup": "^3.1.1",
    "@remix-run/node-fetch-server": "^0.13.0",
    "@shikijs/rehype": "^3.20.0",
    "@shikijs/transformers": "^3.20.0",
    "@shikijs/twoslash": "^3.20.0",
    "@shikijs/types": "^3.20.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@tailwindcss/vite": "^4.1.8",
    "@vitejs/plugin-rsc": "^0.5.9",
    "cva": "npm:class-variance-authority@^0.7.1",
    "esast-util-from-js": "^2.0.1",
    "estree-util-value-to-estree": "^3.5.0",
    "estree-util-visit": "^2.0.0",
    "github-slugger": "^2.0.0",
    "hono": "^4.11.3",
    "jiti": "^2.6.1",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-gfm": "^3.1.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-to-hast": "^13.2.1",
    "mdast-util-to-markdown": "^2.1.2",
    "mdast-util-to-string": "^4.0.0",
    "minisearch": "^7.2.0",
    "react-error-boundary": "^6.0.2",
    "react-router": "^7.11.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-directive": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.1",
    "remark-mdx": "^3.1.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "remark-parse": "^11.0.0",
    "remark-stringify": "^11.0.0",
    "shiki": "^3.20.0",
    "sucrase": "^3.35.1",
    "tailwindcss": "^4.1.8",
    "tailwindcss-logical": "^4.1.0",
    "tsx": "^4.21.0",
    "twoslash": "^0.3.6",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "unplugin-icons": "^22.5.0",
    "vfile": "^6.0.3",
    "yaml": "^2.8.2"
  },
  "peerDependencies": {
    "react": "^19",
    "vite": "^7",
    "waku": "^1"
  },
  "peerDependenciesMeta": {
    "waku": {
      "optional": true
    },
    "vite": {
      "optional": true
    }
  },
  "sideEffects": false,
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "src": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "src": "./src/config.ts",
      "default": "./dist/config.js"
    },
    "./globals": {
      "types": "./dist/globals.d.ts",
      "src": "./src/globals.d.ts"
    },
    "./mdx": {
      "types": "./dist/mdx.d.ts",
      "src": "./src/mdx.tsx",
      "default": "./dist/mdx.js"
    },
    "./waku/internal/routes/*": {
      "types": "./dist/waku/internal/routes/*.d.ts",
      "src": "./src/waku/internal/routes/*.tsx",
      "default": "./dist/waku/internal/routes/*.js"
    },
    "./waku/internal": {
      "types": "./dist/waku/internal/index.d.ts",
      "src": "./src/waku/internal/index.ts",
      "default": "./dist/waku/internal/index.js"
    },
    "./waku/middleware": {
      "types": "./dist/waku/middleware.d.ts",
      "src": "./src/waku/middleware.ts",
      "default": "./dist/waku/middleware.js"
    },
    "./waku/router": {
      "types": "./dist/waku/router.d.ts",
      "src": "./src/waku/router.ts",
      "default": "./dist/waku/router.js"
    },
    "./vite": {
      "types": "./dist/waku/vite.d.ts",
      "src": "./src/waku/vite.ts",
      "default": "./dist/waku/vite.js"
    },
    "./styles/*": {
      "types": "./dist/styles/*.d.ts",
      "src": "./src/styles/*.css",
      "default": "./dist/styles/*.js"
    }
  }
}
